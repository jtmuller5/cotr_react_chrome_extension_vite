{
  "manifest_version": 3,
  "name": "Vite Extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with React, TypeScript, Tailwind CSS, and Vite",
  "permissions": ["sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "options_page": "options.html"
}
